{
    "comments": {
        "blockComment": ["#", "#"]
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["#", "#"],
        ["\"", "\""]
    ],
    "surroundingPairs": [
        ["[", "]"],
        ["#", "#"],
        ["(", ")"],
        ["\"", "\""]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*\\.\\w+\\s*\\{",
            "end": "^\\s*\\}"
        }
    },
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
    "indentationRules": {
        "increaseIndentPattern": "^\\s*.*\\{[^}]*$",
        "decreaseIndentPattern": "^\\s*\\}"
    }
}
{
  "name": "birchlinter",
  "displayName": "BirchLinter",
  "description": "A syntax highlighter for my custom programming language called birch",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kraaven/ProjectBirch"
  },
  "contributes": {
    "languages": [{
      "id": "br",
      "aliases": ["Birch", "br"],
      "extensions": [".br"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "br",
      "scopeName": "source.br",
      "path": "./syntaxes/br.tmLanguage.json"
    }]
  }
}
{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "birch",
      "camelcase": "Birch",
      "title": "Birch",
      "scope": "source.br",
      "file-types": [
        ".br"
      ],
      "injection-regex": "^birch$",
      "class-name": "TreeSitterBirch"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter parser for Birch Programming language",
    "authors": [
      {
        "name": "kraaven"
      }
    ],
    "links": {
      "repository": "https://github.com/Kraaven/ProjectBirch"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
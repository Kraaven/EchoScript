.main{

    # Ask for user age #
    sys-console = "What is your age? :";
    syscall : write;
    syscall : read;
    
    # Take the age and convert into number #
    STR-i = sys-console;
    syscall : toNum;
    num givenAge = NUM-i;
    
    if givenAge > 17 : adult | child;
}

.adult{
    sys-console = "You are an adult!";
    syscall : write;
}

.child{
    sys-console = "Go to bed child";
    syscall : write;
}